<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    
    <!-- Define Desktop Bean First -->
    <bean id="desktop" class="com.tandan.SpringDemo.Desktop"></bean>

    <!-- Define Desktop Bean First -->
    <bean id="com" class="com.tandan.SpringDemo.Laptop" primary="true"></bean>
    <!--primary gives preference to bean in state of confusion-->
    <!-- Define Alien Bean -->
    <bean id="alien" class="com.tandan.SpringDemo.Alien" autowire="byType">
    <property name="age" value="10"></property>
    </bean>
     

</beans>
