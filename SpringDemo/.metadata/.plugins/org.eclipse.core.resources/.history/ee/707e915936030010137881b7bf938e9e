<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Define Laptop Bean First -->
    <bean id="com" class="com.tandan.SpringDemo.Laptop"></bean>
    
    <!-- Define Desktop Bean First -->
    <bean id="com" class="com.tandan.SpringDemo.Desktop"></bean>

    <!-- Define Alien Bean -->
    <bean id="alien" class="com.tandan.SpringDemo.Alien" autowire="byName" scope="prototype">
    <property name="age" value="10"></property>
    </bean>
     

</beans>
